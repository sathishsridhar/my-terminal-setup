syntax on
filetype plugin indent on
set number
set mouse=a
set noswapfile
set tabstop=4
set softtabstop=0 noexpandtab
set shiftwidth=4
set tabstop=8 softtabstop=0 expandtab shiftwidth=4 smarttab
set hlsearch
hi Search ctermbg=Gray
set ignorecase
set backspace=indent,eol,start
let g:ale_set_highlights = 0
map <C-c> "*y
map <C-v> "*p
